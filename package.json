{
  "name": "flow-predictions",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "deploy:contracts": "node src/scripts/deploy.js",
    "deploy:emulator": "FLOW_NETWORK=emulator npm run deploy:contracts",
    "deploy:testnet": "FLOW_NETWORK=testnet npm run deploy:contracts",
    "deploy:mainnet": "FLOW_NETWORK=mainnet npm run deploy:contracts",
    "build:android": "eas build -p android",
    "build:ios": "eas build -p ios",
    "submit:android": "eas submit -p android",
    "submit:ios": "eas submit -p ios",
    "test": "node -e \"require('./src/scripts/testApp').runTests()\""
  },
  "dependencies": {
    "@onflow/fcl": "^1.4.1",
    "@onflow/types": "^1.0.5",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "expo": "~49.0.8",
    "expo-notifications": "~0.20.1",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-vector-icons": "^10.0.0",
    "react-navigation": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "eas-cli": "^3.17.1"
  },
  "private": true
}